{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "JWT": {
    "ValidAudience": "${JWT_VALID_AUDIENCE}",
    "ValidIssuer": "${JWT_VALID_ISSUER}",
    "Secret": "${JWT_SECRET}"
  },
  "EmailSettings": {
    "FromName": "${EMAIL_FROM_NAME}",
    "EmailAddress": "${EMAIL_ADDRESS}",
    "Password": "${EMAIL_PASSWORD}",
    "SMTPServerHost": "smtp.yandex.ru",
    "SMTPServerPort": "465"
  },
  "Authentication": {
    "Google": {
      "ClientId": "$",
      "ClientSecret": "$",
      "RedirectUrl": "$"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "${DATABASE_CONNECTION_STRING}",
    "RedisConnection": "${REDIS_CONNECTION_STRING}"
  },
  "MinioS3": {
    "MinioClient": "{S3_CLIENT_NAME}",
    "AccessKey": "${S3_ACCESS_KEY}",
    "SecretKey": "${S3_SECRET_KEY}",
    "ServiceUrl": "${S3_URL}",
    "BucketName": "${S3_BUCKET_NAME}"
  }, 
  "RabbitMq": {
      "Host": "localhost"
  },
  "Hangfire": {
    "DisplayDashBoard": false,
    "CronForSendNotificationSubscribe": "0 7 * * *",
    "CronForSendEmailNotificator": "*/15 * * * *"
  },
  "AllowedHosts": "*",
  "RedirectUrl": "*://**:**/"
}
