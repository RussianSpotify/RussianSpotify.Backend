{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MinioS3": {
    "MinioClient": "{S3_CLIENT_NAME}",
    "AccessKey": "${S3_ACCESS_KEY}",
    "SecretKey": "${S3_SECRET_KEY}",
    "ServiceUrl": "${S3_URL}",
    "BucketName": "${S3_BUCKET_NAME}",
    "TempBucketName": "${S3_TEMP_BUCKET_NAME}"
  },
  "ConnectionStrings": {
    "RedisConnection": "redis:6379:"
  }
}
